{
  "name": "@osu-cass/sb-components",
  "version": "0.0.31",
  "description": "Shared components for Smarter Balanced",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "webpack-prod": "./node_modules/.bin/webpack --config webpack.config.js --env.prod",
    "webpack-dev": "./node_modules/.bin/webpack --config webpack.config.js --env.dev",
    "prepare": "npm run clean && npm run test && npm run webpack-prod && cp -r ./src/Styles ./lib/Styles",
    "precommit": "lint-staged",
    "start": "npm run storybook",
    "test": "./node_modules/.bin/jest",
    "storybook": "npm run clean && start-storybook -p 6006",
    "build-storybook": "npm run clean && build-storybook",
    "storybook-publish": "build-storybook -o storybuild",
    "build": "npm run clean && build-storybook && npm run webpack-dev",
    "clean": "rm -rf ./lib ?",
    "prettier": "prettier --write \"src/*/*.ts\" && prettier --write \"src/*/*.tsx\" && prettier --write \"stories/*/*.tsx\""
  },
  "lint-staged": {
    "*.tsx": ["npm run prettier","git add"],
    "*.ts": ["npm run prettier","git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osu-cass/sb-components.git"
  },
  "authors": [
    "Dan Van Horn (danvanhorn)",
    "Kevin Turkington (zainkai)"
  ],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/osu-cass/sb-components/issues"
  },
  "homepage": "https://github.com/osu-cass/sb-components#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-storyshots": "^3.2.12",
    "@storybook/addons": "^3.2.12",
    "@storybook/channels": "^3.2.12",
    "@storybook/cli": "~3.2.12",
    "@storybook/react": "~3.2.12",
    "@types/bootstrap": "3.3.32",
    "@types/enzyme": "^3.1.4",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/google.analytics": "0.0.29",
    "@types/jest": "~21.1.2",
    "@types/jquery": "2.0.40",
    "@types/node": "~8.0.34",
    "@types/react": "~16.0.10",
    "@types/react-dom": "~16.0.1",
    "@types/react-modal": "2.2.2",
    "@types/react-router": "4.0.12",
    "@types/react-router-dom": "4.0.5",
    "@types/storybook__addon-actions": "~3.0.1",
    "@types/storybook__react": "~3.0.5",
    "awesome-typescript-loader": "^3.4.0",
    "bootstrap": "3.3.7",
    "css-loader": "~0.28.7",
    "dts-generator": "^2.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "~4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "font-awesome": "~4.7.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~21.2.1",
    "jest-cli": "~21.2.1",
    "jquery": "3.2.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "less": "~2.7.3",
    "less-loader": "~4.0.5",
    "postcss-loader": "~2.0.8",
    "prettier": "~1.8.2",
    "react": "~16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "~16.0.0",
    "react-router-dom": "4.1.1",
    "react-test-renderer": "~16.0.0",
    "style-loader": "~0.19.0",
    "ts-jest": "~21.1.2",
    "ts-loader": "^3.1.1",
    "typescript": "~2.5.3",
    "typescript-eslint-parser": "~8.0.1",
    "url-loader": "0.5.9",
    "webpack": "~3.8.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTest.ts",
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(stories|src)/.*(test)\\.(js|ts|tsx?)$",
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/**/__tests__/*",
      "!src/index.ts",
      "!**/node_modules/**"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "json",
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "identity-obj-proxy",
      "^.+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "react-modal": "3.1.2"
  }
}
